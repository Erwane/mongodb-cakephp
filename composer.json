{
    "name": "shockraver/mongodb",
    "description": "An Mongodb datasource for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=8.1",
        "ext-mongodb": "*",
        "cakephp/cakephp": "^5.0",
        "mongodb/mongodb": "^1.20"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5.5 || ^11.1.3"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "giginc",
            "email": "dev@giginc.co.jp"
        }
    ],
    "autoload": {
        "psr-4": {
            "Giginc\\Mongodb\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Giginc\\Mongodb\\Test\\": "tests"
        }
    }
}
